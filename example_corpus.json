{
    "corpus": "Dannes Superkorpus",
    "word_segmenter": "punkt_word",
    "root_tag":
    { "tag": "text",
      "attributes": ["title"]
    },
    "sentence_segmenter":
    { "tag": "s",
      "attributes": ["number"]
    },
    "paragraph_segmenter":
    { "tag": "p",
      "attributes": ["name"]
    },
    "extra_tags":
    [
        { "tag": "chapter",
          "attributes": ["name"]
        }
    ],
    "attributes": ["word", "pos", "msd", "lemma", "lex", "saldo", "prefix", "suffix", "ref", "dephead", "deprel"]
}
